{
  "name": "api-feature-flags",
  "version": "0.0.1",
  "main": "dist/server.js",
  "type": "module",
  "files": [
    "dist/src"
  ],
  "exports": {
    "./api-routes": {
      "import": "./dist/src/api-routes.js"
    },
    "./server": {
      "import": "./dist/src/server.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "db:reset": "dropdb feature_flags && createdb feature_flags && npm run db:setup",
    "db:seed": "psql feature_flags < src/seed-data.sql",
    "db:setup": "psql feature_flags < src/schema.sql",
    "dev": "tsx src/server.ts",
    "start": "node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "pg": "^8.11.3",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "socket.io": "^4.6.1",
    "socket.io-client": "^4.8.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "@types/pg": "^8.10.9",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0"
  }
}
